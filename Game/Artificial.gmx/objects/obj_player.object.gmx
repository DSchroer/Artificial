{
    inventory[selected_slot] = player_update_gun(inventory[selected_slot]);
}

if(!dead)
{
    gun_angle = point_direction(x, y - 64, mouse_x, mouse_y);
}

gun_recoil = min(gun_recoil, gun_base_dist);
gun_dist = gun_base_dist - gun_recoil;
if(gun_recoil &gt; 0)
{
    gun_recoil-=5;
    gun_recoil = max(gun_recoil, 0);
}

player_update_shield();
debug_show_string("fps: " + string(fps));
debug_show_string("stub: " + level_stub_name());

if(speed != 0)
{
    dash_last_direction = direction;
}
if(keymap_check(keycode.dash) &amp;&amp; !dead &amp;&amp; dash_frames == 0 &amp;&amp; !window_locked)
{
    dash_frames = 15;
    dash_direction = dash_last_direction;
}

if(dash_frames &gt; 0)
{
    dash_frames -= 1;
    motion_add(dash_direction, dash_speed);
    if(dash_frames = 0)
    {
        dash_frames = -60;
    }
}else if(dash_frames &lt; 0)
{
    dash_frames += 1;
}

if(keymap_check(keycode.interact) &amp;&amp; !dead &amp;&amp; !window_locked)
{
    var inter = interact_any(128);
    if(inter == noone)
    {
        var obj = instance_place(x, y, obj_item);
        var data = -1;
        if(obj)
        {
            data = obj.data;
        }
        var out = noone;
        if(is_gun(data))
        {
            out = inventory_pickup_weapon(data);
        }else if(is_shield(data))
        {
            out = -1;
            player_activate_shield();
        }
        
        with(obj)
        {
            instance_destroy();
        }
        if(out != -1 &amp;&amp; is_array(out))
        {
            var item = instance_create(x,y, obj_item);
            item.data = out;
        }
    
    }else{
        interact_trigger(inter);
    }
}

if(invincible &gt; 0)
{
    --invincible;
}

if(!open_workbench &amp;&amp; !window_locked )
{   
    if(keymap_check(keycode.reload))
    {
        inventory[selected_slot] = player_reload(inventory[selected_slot]);
    }

}

if(keymap_check(keycode.escape))
{
    if(!is_paused())
    {
        if(journal_is_open)
        {
            journal_is_open = false; 
        }
        else if(open_workbench)
        {
            open_workbench = false;
        }
        else if(room != rm_transition)
        {
            instance_create(0,0,obj_pause);
            open_workbench = false;
            journal_is_open = false;
        }
    }
}

entity_collision();

player_set_sprite();

player_render_self();


// Component tutorial
if(!save_get_value(obj_save_data, "components_tutorial", false) &amp;&amp; save_get_value(obj_save_data, "components", 0) &gt; 0)
